<h1>Feedback for user</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">

    <table class="table table-striped ">
      <thead>
      <tr>
        <td>Event</td>
        <td>Date</td>
      </tr>
      </thead>
      <tbody>

      <% @feedbacks.each do |feedback| %>
          <tr>
            <td><%= feedback.name %></td>
            <td><%= feedback.comments %></td>
            <td><%= feedback.rating %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>
</div>