<h1>Add Talks for an event</h1>


<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@talk) do |f| %>

        <div class="form-group">
          <%= f.label :title %>
          <%= f.text_field :title, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.label :presenters %>
          <%= f.date_select :presenters, class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.submit "Create new event", class: "btn btn-default" %>
        </div>
    <% end %>
  </div>
</div>